{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "materialOuServico": {
      "type": "string",
      "enum": ["M","S"]
    },
    "tipoBeneficioId": {
      "type": "string"
    },
    "incentivoProdutivoBasico": {
      "type": "boolean"
    },
    "descricao": {
      "type": "string",
      "maxLength": 2048
    },
    "quantidade": {
      "type": "integer"
    },
    "unidadeMedida": {
      "type": "string",
      "maxLength": 30
    },
    "valorUnitarioEstimado": {
      "type": "number"
    },
    "valorTotal": {
      "type": "number"
    },
    "criterioJulgamentoId": {
      "type": "integer",
      "enum": [1,2,4,5,6,7,8,9]
    },
    "orcamentoSigiloso": {
      "type": "boolean"
    },
    "itemCategoriaId": {
      "type": "integer",
      "enum": [1,2,3]
    },
    "patrimonio": {
      "type": "string",
      "maxLength": 255
    },
    "codigoRegistroImobiliario": {
      "type": "string",
      "maxLength": 255
    },
    "aplicabilidadeMargemPreferenciaNormal": {
      "type": "boolean"
    },
    "aplicabilidadeMargemPreferenciaAdicional": {
      "type": "boolean"
    },
    "percentualMargemPreferenciaNormal": {
      "type": ["number", "null"],
      "minimum": 0,
      "maximum": 100
    },
    "percentualMargemPreferenciaAdicional": {
      "type": ["number", "null"],
      "minimum": 0,
      "maximum": 100
    },
    "ncmNbsCodigo": {
      "type": "string",
      "maxLength": 15
    },
    "ncmNbsDescricao": {
      "type": "string",
      "maxLength": 2048
    },
    "catalogoId": {
      "type": "integer",
      "enum": [1,2]
    },
    "categoriaItemCatalogoId": {
      "type": "integer",
      "enum": [1,2,3,4,5,6,7,8]
    },
    "catalogoCodigoItem": {
      "type": "string",
      "maxLength": 20
    },
    "informacaoComplementar": {
      "type": "string",
      "maxLength": 4096
    },
    "codigoTipoMargemPreferencia": {
      "type": "integer",
      "enum": [1,2]
    },
    "inConteudoNacional": {
      "type": "boolean"
    },
    "justificativa": {
      "type": "string",
      "minLength": 15,
      "maxLength": 255
    }
  },
  "required": [
    "justificativa"
  ]
}